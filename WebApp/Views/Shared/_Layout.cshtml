    <script src="~/front/assets/js/popper.min.js" asp-append-version="true"></script>
    <script src="~/front/assets/js/bootstrap.min.js" asp-append-version="true"></script>
    <script src="~/front/assets/js/notify.js" asp-append-version="true"></script>
    <script src="~/front/assets/js/owl.carousel.min.js" asp-append-version="true"></script>
    <script src="~/front/assets/js/isotope.pkgd.min.js" asp-append-version="true"></script>
    <script src="~/front/assets/js/one-page-nav-min.js" asp-append-version="true"></script>
